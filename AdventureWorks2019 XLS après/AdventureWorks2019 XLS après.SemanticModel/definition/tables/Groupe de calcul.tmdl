table 'Groupe de calcul'
	lineageTag: 612e4c39-2285-4e92-b464-d25119d95f9c

	calculationGroup

		calculationItem 'Rolling 12 Month' = ```
				
				VAR SelectedDate = MAX('Date'[Date]) 
				VAR PreviousDate = DATESINPERIOD(Date2[Date], SelectedDate, -12, MONTH) 
				
				RETURN  
				    CALCULATE(
				        SELECTEDMEASURE()
				        , REMOVEFILTERS('Date')
				        , KEEPFILTERS(PreviousDate)
				        , USERELATIONSHIP(Date2[DateKey],'Date'[DateKey])
				    )
				    * 4
				```

		calculationItem 'Calculation item' = SELECTEDMEASURE()

